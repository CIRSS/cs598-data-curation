<?xml version="1.0"?>
<rdf:RDF xmlns="http://www.semantic.org/cs598/frbr/"
     xml:base="http://www.semantic.org/cs598/frbr/"
     xmlns:owl="http://www.w3.org/2002/07/owl#"
     xmlns:rdf="http://www.w3.org/1999/02/22-rdf-syntax-ns#"
     xmlns:xsd="http://www.w3.org/2001/XMLSchema#"
     xmlns:frbr="http://www.semantic.org/cs598/frbr#"
     xmlns:rdfs="http://www.w3.org/2000/01/rdf-schema#">

    <!-- Ontology metadata -->
    <owl:Ontology rdf:about="">
        <rdfs:label>Example FRBR Ontology for CS598</rdfs:label>
        <rdfs:comment>
          Teaching example demonstrating FRBR Group 1 (Work, Expression, Manifestation, Item) 
          and Group 2 (Person, CorporateBody) entities.
        </rdfs:comment>
        <owl:versionInfo>1.1</owl:versionInfo>
    </owl:Ontology>

    <!-- Object Properties -->

    <owl:ObjectProperty rdf:about="frbr:isCreatedBy">
        <rdfs:domain rdf:resource="frbr:Endeavor"/>
        <rdfs:range rdf:resource="frbr:ResponsibleEntity"/>
        <owl:inverseOf rdf:resource="frbr:creates"/>
    </owl:ObjectProperty>

    <owl:ObjectProperty rdf:about="frbr:creates">
        <rdfs:domain rdf:resource="frbr:ResponsibleEntity"/>
        <rdfs:range rdf:resource="frbr:Endeavor"/>
    </owl:ObjectProperty>

    <owl:ObjectProperty rdf:about="frbr:isRealizedBy">
        <rdfs:domain rdf:resource="frbr:Work"/>
        <rdfs:range rdf:resource="frbr:Expression"/>
    </owl:ObjectProperty>

    <owl:ObjectProperty rdf:about="frbr:isEmbodiedBy">
        <rdfs:domain rdf:resource="frbr:Expression"/>
        <rdfs:range rdf:resource="frbr:Manifestation"/>
    </owl:ObjectProperty>

    <owl:ObjectProperty rdf:about="frbr:isExemplifiedBy">
        <rdfs:domain rdf:resource="frbr:Manifestation"/>
        <rdfs:range rdf:resource="frbr:Item"/>
    </owl:ObjectProperty>

    <owl:ObjectProperty rdf:about="frbr:isHeldBy">
        <rdfs:domain rdf:resource="frbr:Item"/>
        <rdfs:range rdf:resource="frbr:Library"/>
        <owl:inverseOf rdf:resource="frbr:holds"/>
    </owl:ObjectProperty>

    <owl:ObjectProperty rdf:about="frbr:holds">
        <rdfs:domain rdf:resource="frbr:Library"/>
        <rdfs:range rdf:resource="frbr:Item"/>
    </owl:ObjectProperty>

    <!-- Datatype properties -->

    <owl:DatatypeProperty rdf:about="frbr:title">
        <rdfs:domain rdf:resource="frbr:Endeavor"/>
        <rdfs:range rdf:resource="xsd:string"/>
    </owl:DatatypeProperty>

    <owl:DatatypeProperty rdf:about="frbr:name">
        <rdfs:domain rdf:resource="frbr:ResponsibleEntity"/>
        <rdfs:range rdf:resource="xsd:string"/>
    </owl:DatatypeProperty>

    <!-- Classes -->

    <owl:Class rdf:about="frbr:Endeavor"/>
    <owl:Class rdf:about="frbr:Work">
        <rdfs:subClassOf rdf:resource="frbr:Endeavor"/>
    </owl:Class>
    <owl:Class rdf:about="frbr:Expression">
        <rdfs:subClassOf rdf:resource="frbr:Endeavor"/>
    </owl:Class>
    <owl:Class rdf:about="frbr:Manifestation">
        <rdfs:subClassOf rdf:resource="frbr:Endeavor"/>
    </owl:Class>
    <owl:Class rdf:about="frbr:Item">
        <rdfs:subClassOf rdf:resource="frbr:Endeavor"/>
    </owl:Class>

    <owl:Class rdf:about="frbr:ResponsibleEntity"/>
    <owl:Class rdf:about="frbr:Person">
        <rdfs:subClassOf rdf:resource="frbr:ResponsibleEntity"/>
    </owl:Class>
    <owl:Class rdf:about="frbr:CorporateBody">
        <rdfs:subClassOf rdf:resource="frbr:ResponsibleEntity"/>
    </owl:Class>

    <owl:Class rdf:about="frbr:Library"/>
    <owl:Class rdf:about="frbr:AcademicLibrary">
        <rdfs:subClassOf rdf:resource="frbr:Library"/>
    </owl:Class>
    <owl:Class rdf:about="frbr:PublicLibrary">
        <rdfs:subClassOf rdf:resource="frbr:Library"/>
    </owl:Class>

    <!-- Individuals -->

    <owl:NamedIndividual rdf:about="frbr:WilliamShakespeare">
        <rdf:type rdf:resource="frbr:Person"/>
        <frbr:name>Shakespeare, William</frbr:name>
    </owl:NamedIndividual>

    <owl:NamedIndividual rdf:about="frbr:ShakespearesHamlet">
        <rdf:type rdf:resource="frbr:Work"/>
        <frbr:isRealizedBy rdf:resource="frbr:HamletExpression"/>
        <frbr:title>The Tragedy of Hamlet, Prince of Denmark</frbr:title>
    </owl:NamedIndividual>

    <owl:NamedIndividual rdf:about="frbr:HamletExpression">
        <rdf:type rdf:resource="frbr:Expression"/>
        <frbr:isEmbodiedBy rdf:resource="frbr:BranaghsHamlet"/>
        <frbr:isEmbodiedBy rdf:resource="frbr:GidesFrenchHamlet"/>
    </owl:NamedIndividual>

    <owl:NamedIndividual rdf:about="frbr:BranaghsHamlet">
        <rdf:type rdf:resource="frbr:Manifestation"/>
        <frbr:isExemplifiedBy rdf:resource="frbr:Hamlet1996DVD"/>
        <frbr:isCreatedBy rdf:resource="frbr:KennethBranagh"/>
        <frbr:title>Hamlet (1996 film)</frbr:title>
    </owl:NamedIndividual>

    <owl:NamedIndividual rdf:about="frbr:Hamlet1996DVD">
        <rdf:type rdf:resource="frbr:Item"/>
        <frbr:isHeldBy rdf:resource="frbr:MusicLibrary"/>
        <frbr:title>Hamlet DVD copy</frbr:title>
    </owl:NamedIndividual>

    <owl:NamedIndividual rdf:about="frbr:GidesFrenchHamlet">
        <rdf:type rdf:resource="frbr:Manifestation"/>
        <frbr:isExemplifiedBy rdf:resource="frbr:PantheonBooksFrenchHamlet"/>
        <frbr:isCreatedBy rdf:resource="frbr:AndreGide"/>
        <frbr:title>Hamlet (French translation)</frbr:title>
    </owl:NamedIndividual>

    <owl:NamedIndividual rdf:about="frbr:PantheonBooksFrenchHamlet">
        <rdf:type rdf:resource="frbr:Item"/>
        <frbr:isHeldBy rdf:resource="frbr:MainLibrary"/>
        <frbr:title>Hamlet â€“ French edition</frbr:title>
    </owl:NamedIndividual>

    <owl:NamedIndividual rdf:about="frbr:KennethBranagh">
        <rdf:type rdf:resource="frbr:Person"/>
        <frbr:name>Branagh, Kenneth</frbr:name>
    </owl:NamedIndividual>

    <owl:NamedIndividual rdf:about="frbr:AndreGide">
        <rdf:type rdf:resource="frbr:Person"/>
        <frbr:name>Andre Gide</frbr:name>
    </owl:NamedIndividual>

    <owl:NamedIndividual rdf:about="frbr:PantheonBooks">
        <rdf:type rdf:resource="frbr:CorporateBody"/>
        <frbr:name>Pantheon Books</frbr:name>
    </owl:NamedIndividual>

    <owl:NamedIndividual rdf:about="frbr:MainLibrary">
        <rdf:type rdf:resource="frbr:AcademicLibrary"/>
        <rdfs:label>Main Library</rdfs:label>
    </owl:NamedIndividual>

    <owl:NamedIndividual rdf:about="frbr:MusicLibrary">
        <rdf:type rdf:resource="frbr:AcademicLibrary"/>
        <rdfs:label>Music Library</rdfs:label>
    </owl:NamedIndividual>

</rdf:RDF>
